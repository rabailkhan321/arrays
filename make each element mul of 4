#include<stdio.h>
  int multiple(int a[], int x);


int main()
{
    int a[30];
    int i;
    printf("enter the elements of array");
    for(i=0; i<5; i++)
    {
        scanf("%d", &a[i]);
    }
    multiple(a,5 );
    for(i=0; i<5; i++)
    {
        printf("array of multiple of 4 is %d", a[i]);
    }
}
  int multiple(int a[], int x)
  {

      int i, mul=4;
       for(i=0; i<x; i++)
       {

        a[i]=a[i]*4;

           }



        return a[i];
       }

 int is_odd(int a[],int x)
 {
 int i, odd=0;
       for(i=0; i<x; i++)
       {

        if(a[i]%2!=0)
        odd++;

           }



        return odd;
       }
