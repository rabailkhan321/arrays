#include<stdio.h>
int main()
{
    int a[100];
    for(int i=0; i<100; i++)
    {
        if(i<30)
        {
            a[i]=1;
        }
        else
        {
            a[i]=0;
        }
    }
    for(int i=0; i<100; i++)
    {
        printf("the element of array %d is %d\n", i, a[i]);
    }
}
