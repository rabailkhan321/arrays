#include<stdio.h>
int fun(int arr[]);

int main()
{
    int i;
    int arr[10];
    i=fun(&arr[0]);
    printf("the  total no ofeven no are %d\n", i);
}
int fun(int arr[])
{
    int even=0;
    int i;
  
    printf("enter the elements of  array");
    for( i=0; i<10; i++){
    scanf("%d",&arr[i]);
    if(arr[i]%2==0)
    even++;}
    return even;
}
